[user]
    name = My Name
    email = my@example.com
[color]
    ui = true
[alias]
    st = status -sb
    co = checkout
    ci = commit
    br = branch
    l = log --graph --full-history --pretty=format:'%C(red)%h%C(reset) - %C(yellow)%C(yellow)%s %C(green)(%an, %cr)%C(reset)' --abbrev-commit --date=relative
    contains = branch --contains
[push]
    default = current
[diff "po"]
    textconv=msgcat --no-location --no-wrap --sort-output
